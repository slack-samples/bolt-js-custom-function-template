{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "object-curly-newline": "off",
        "camelcase": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    }
}
